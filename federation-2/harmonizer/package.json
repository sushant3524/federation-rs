{
  "name": "@apollo/harmonizer-2",
  "private": true,
  "version": "https://pkg.csb.dev/apollographql/federation/commit/ddaf0dd2/@apollo/composition",
  "description": "Apollo Federation Harmonizer JS Entrypoint",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "make-dir bundled js-dist && rm -f tsconfig.tsbuildinfo && tsc --build --verbose && node esbuild/bundler.js && cp js-dist/runtime.js js-dist/do_compose.js bundled/ && cp node_modules/fake-composition-wasm/fake_composition_wasm_bg.wasm bundled/",
    "clean": "rm -rf ./node_modules ./js-dist ./bundled ./tsconfig.tsbuildinfo",
    "lint": "prettier --check ./esbuild/**/*.js ./js-src/**/*.ts ./js-src/**/*.js",
    "format": "prettier --write ./esbuild/**/*.js ./js-src/**/*.ts ./js-src/**/*.js"
  },
  "browser": {
    "util": "util",
    "url": "universal-url",
    "console": "console-polyfill",
    "path": "path-browserify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apollographql/federation-rs.git",
    "directory": "harmonizer/src"
  },
  "author": "Apollo <packages@apollographql.com>",
  "license": "SEE LICENSE IN ./LICENSE",
  "engines": {
    "node": ">=12.13.0 <17.0",
    "npm": ">=7 <10"
  },
  "dependencies": {
    "@apollo/composition": "https://pkg.csb.dev/apollographql/federation/commit/ddaf0dd2/@apollo/composition",
    "@apollo/federation-internals": "https://pkg.csb.dev/apollographql/federation/commit/ddaf0dd2/@apollo/federation-internals",
    "@apollo/gateway": "https://pkg.csb.dev/apollographql/federation/commit/ddaf0dd2/@apollo/gateway",
    "@apollo/query-graphs": "https://pkg.csb.dev/apollographql/federation/commit/ddaf0dd2/@apollo/query-graphs",
    "@apollo/query-planner": "https://pkg.csb.dev/apollographql/federation/commit/ddaf0dd2/@apollo/query-planner",
    "@apollo/subgraph": "https://pkg.csb.dev/apollographql/federation/commit/ddaf0dd2/@apollo/subgraph",
    "fake-composition-wasm": "https://pkg.csb.dev/apollographql/federation/commit/ddaf0dd2/fake-composition-wasm"
  },
  "peerDependencies": {
    "graphql": "^15.7.0 || ^16.0.0"
  },
  "devDependencies": {
    "@iarna/toml": "2.2.5",
    "buffer": "^6.0.3",
    "console-polyfill": "^0.3.0",
    "esbuild": "^0.17.0",
    "make-dir-cli": "^3.0.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.5.1",
    "typescript": "^5.0.2",
    "universal-url": "^2.0.0",
    "util": "^0.12.4"
  },
  "volta": {
    "node": "16.20.0",
    "npm": "9.7.1"
  }
}